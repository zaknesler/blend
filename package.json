{
  "name": "blend-ui",
  "type": "module",
  "version": "0.0.0",
  "author": "Zak Nesler <zak@nesler.dev> (https://nesler.dev)",
  "packageManager": "bun@1.1.20",
  "engines": { "node": ">=18" },
  "scripts": {
    "dev": "vite ui",
    "build": "vite build ui",
    "serve": "vite preview ui",
    "ci": "biome ci ./ui/src",
    "lint": "biome check ./ui/src",
    "lint:fix": "biome check --write ./ui/src",
    "test": "vitest run --root ui",
    "test:watch": "vitest watch --root ui",
    "test:cov": "vitest run --root ui --coverage",
    "watch": "mprocs \"bun run dev\" \"cargo watch -x \\\"run -- start\\\"\"",
    "watch:trace": "mprocs \"bun run dev\" \"cargo watch -x \\\"run -- -ltrace start\\\"\"",
    "typeshare:generate": "typeshare ./ --lang typescript --output-file ./ui/src/types/bindings.ts && biome check --write ./ui/src/types/bindings.ts"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.5",
    "@kobalte/tailwindcss": "^0.9.0",
    "@solid-primitives/active-element": "^2.0.20",
    "@solid-primitives/bounds": "^0.0.122",
    "@solid-primitives/keyboard": "^1.2.8",
    "@solid-primitives/resize-observer": "^2.0.26",
    "@solid-primitives/scheduled": "^1.4.3",
    "@solid-primitives/scroll": "^2.0.23",
    "@solidjs/router": "^0.14.3",
    "@tanstack/solid-query": "^5.52.2",
    "@tanstack/solid-query-devtools": "^5.52.2",
    "class-variance-authority": "^0.7.0",
    "partysocket": "^1.0.2",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.21",
    "solid-transition-group": "^0.2.3",
    "wretch": "^2.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@total-typescript/ts-reset": "^0.6.0",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.41",
    "solid-devtools": "^0.30.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
