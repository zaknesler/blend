{
  "name": "blend-ui",
  "type": "module",
  "scripts": {
    "dev": "vite ui",
    "build": "vite build ui",
    "serve": "vite preview ui",
    "ci": "biome ci ./ui/src",
    "lint": "biome check ./ui/src",
    "lint:fix": "biome check --write ./ui/src",
    "test": "vitest run --root ui",
    "test:watch": "vitest watch --root ui",
    "test:cov": "vitest run --root ui --coverage",
    "watch": "mprocs \"pnpm dev\" \"cargo watch -x \\\"run -- start\\\"\"",
    "watch:trace": "mprocs \"pnpm dev\" \"cargo watch -x \\\"run -- -ltrace start\\\"\"",
    "typeshare:generate": "typeshare ./ --lang typescript --output-file ./ui/src/types/bindings.ts && biome check --write ./ui/src/types/bindings.ts"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.3",
    "@kobalte/tailwindcss": "^0.9.0",
    "@solid-primitives/active-element": "^2.0.20",
    "@solid-primitives/bounds": "^0.0.121",
    "@solid-primitives/keyboard": "^1.2.8",
    "@solid-primitives/resize-observer": "^2.0.25",
    "@solid-primitives/scheduled": "^1.4.3",
    "@solid-primitives/scroll": "^2.0.23",
    "@solidjs/router": "^0.13.6",
    "@tanstack/solid-query": "^5.49.1",
    "@tanstack/solid-query-devtools": "^5.49.1",
    "class-variance-authority": "^0.7.0",
    "partysocket": "^1.0.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.18",
    "solid-transition-group": "^0.2.3",
    "wretch": "^2.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@total-typescript/ts-reset": "^0.5.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "solid-devtools": "^0.30.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18"
  }
}
