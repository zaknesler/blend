{
  "name": "blend-ui",
  "type": "module",
  "version": "0.0.0",
  "author": "Zak Nesler <zak@nesler.dev> (https://nesler.dev)",
  "packageManager": "bun@1.1.20",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite ui",
    "build": "vite build ui",
    "serve": "vite preview ui",
    "ci": "biome ci ./ui/src",
    "lint": "biome check ./ui/src",
    "lint:fix": "biome check --write ./ui/src",
    "test": "vitest run --root ui",
    "test:watch": "vitest watch --root ui",
    "test:cov": "vitest run --root ui --coverage",
    "watch": "mprocs \"bun run dev\" \"cargo watch -x \\\"run -- start\\\"\"",
    "watch:trace": "mprocs \"bun run dev\" \"cargo watch -x \\\"run -- -ltrace start\\\"\"",
    "typeshare:generate": "typeshare ./ --lang typescript --output-file ./ui/src/types/bindings.ts && biome check --write ./ui/src/types/bindings.ts"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@kobalte/tailwindcss": "^0.9.0",
    "@solid-primitives/active-element": "^2.1.0",
    "@solid-primitives/bounds": "^0.1.0",
    "@solid-primitives/keyboard": "^1.3.0",
    "@solid-primitives/resize-observer": "^2.1.0",
    "@solid-primitives/scheduled": "^1.5.0",
    "@solid-primitives/scroll": "^2.1.0",
    "@solidjs/router": "^0.15.3",
    "@tanstack/solid-query": "^5.67.1",
    "@tanstack/solid-query-devtools": "^5.67.1",
    "class-variance-authority": "^0.7.1",
    "partysocket": "^1.0.3",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.5",
    "solid-transition-group": "^0.3.0",
    "wretch": "^2.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.11",
    "@total-typescript/ts-reset": "^0.6.1",
    "@vitest/coverage-v8": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "solid-devtools": "^0.33.0",
    "tailwindcss": "^4.0.11",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  }
}
